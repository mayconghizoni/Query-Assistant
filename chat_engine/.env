# QUERY_SQL="SELECT * FROM NeoUser"
QUERY_SQL="SELECT DISTINCT
    p.code AS PROCESS_CODE,
    CAST(DATE_FORMAT(p.startDate, '%d/%m/%Y') AS CHAR(10)) AS PROCESS_START_DATE,
    CAST(DATE_FORMAT(p.finishDate, '%d/%m/%Y') AS CHAR(10)) AS PROCESS_FINISH_DATE,
    pn.ValorTotalDeFaturamento AS ValorTotalDeFaturamento,
    pn.AprovarProposta AS AprovarProposta,
    pn.SolicitacaoDeclinadaPeloMotivo AS SolicitacaoDeclinadaPeloMotivo,
    pn.Aviso AS Aviso,
    pn.FornecerDesconto AS FornecerDesconto,
    pn.ValorDoDesconto AS ValorDoDesconto,
    pn.PrevisaoDeEntrega AS PrevisaoDeEntrega,
    pn.DataDeenvio AS DataDeenvio,
    pn.EntregaEmAtrasoPor AS EntregaEmAtrasoPor,
	pn.Empresa AS Empresa,
	pn.RamoDaEmpresa AS RamoDaEmpresa,
	pn.Produto AS Produto,
	pn.Budget AS Budget,
	pn.ValorOrcado AS ValorOrcado,
	pn.Quantidade AS Quantidade,

    CASE p.processState
        WHEN 0 THEN 'Executing'
        WHEN 1 THEN 'Completed'
        WHEN 2 THEN 'Canceled'
        WHEN 3 THEN 'Suspended'
        WHEN 4 THEN 'Terminated'
        ELSE 'Without status'
    END AS PROCESS_STATUS

FROM
    WFProcess p
LEFT JOIN ProcessModel pm ON pm.neoId = p.model_neoId
LEFT JOIN D_FRSolicitacaoDeManutencao pn ON pn.neoId = p.entity_neoId

WHERE
    p.saved = 1"